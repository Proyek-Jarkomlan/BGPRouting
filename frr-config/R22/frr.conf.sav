frr version 8.1
frr defaults traditional
hostname R22
domainname 
no ipv6 forwarding
service integrated-vtysh-config
!
interface r22-eth0
 ip address 192.168.2.2/29
 ip ospf area 0
exit
!
interface r22-eth1
 ip address 10.2.3.2/29
 ip ospf area 0
exit
!
interface r22-eth2
 ip address 10.2.1.2/29
 ip ospf area 0
exit
!
