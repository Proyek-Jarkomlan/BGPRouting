frr version 8.1
frr defaults traditional
hostname R14
domainname 
no ipv6 forwarding
service integrated-vtysh-config
!
interface r14-eth0
 ip address 10.1.1.1/29
 ip ospf area 0
exit
!
interface r14-eth1
 ip address 10.1.4.1/29
 ip ospf area 0
exit
!
interface r14-eth2
 ip address 192.168.2.1/29
 ip ospf area 0
exit
!
