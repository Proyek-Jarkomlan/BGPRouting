frr version 8.1
frr defaults traditional
hostname R21
domainname 
no ipv6 forwarding
service integrated-vtysh-config
!
interface r21-eth0
 ip address 172.16.2.1/29
 ip ospf area 0
exit
!
interface r21-eth1
 ip address 10.2.1.1/29
 ip ospf area 0
exit
!
interface r21-eth2
 ip address 10.2.2.1/29
 ip ospf area 0
exit
!
