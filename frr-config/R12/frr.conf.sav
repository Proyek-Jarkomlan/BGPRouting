frr version 8.1
frr defaults traditional
hostname R12
domainname 
no ipv6 forwarding
service integrated-vtysh-config
!
interface r12-eth0
 ip address 172.16.1.1/29
 ip ospf area 0
exit
!
interface r12-eth1
 ip address 10.1.1.2/29
 ip ospf area 0
exit
!
interface r12-eth2
 ip address 10.1.2.2/29
 ip ospf area 0
exit
!
